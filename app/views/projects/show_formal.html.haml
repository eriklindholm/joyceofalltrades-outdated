.project_page_container.project_formal
  .banner{style: "background-image: url(#{@project.main_image})"}
  .tag_list
    = @project.tags ' - '
  .project_content
    = raw @project.body
  .project_images_list
    - @project.project_images.each do |project_image|
      .project_image= image_tag project_image.image
